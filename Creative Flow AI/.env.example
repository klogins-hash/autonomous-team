```bash
# Environment Configuration
ENVIRONMENT=development

# Database
DB_USER=creative_flow
DB_PASSWORD=dev_password
DB_NAME=creative_flow_db
DB_HOST=postgres
DB_PORT=5432

# Redis
REDIS_PASSWORD=dev_password
REDIS_HOST=redis
REDIS_PORT=6379

# API Configuration
SECRET_KEY=your_secret_key_here_change_in_production
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Frontend
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000

# AI/ML
AI_MODEL_PATH=/models
ENABLE_GPU=false
BATCH_SIZE=32
LEARNING_RATE=0.001

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Email (Optional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# AWS/S3 (Optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET=creative-flow-assets

# Voice Interface
ENABLE_VOICE=true
VOICE_PROVIDER=google  # google, azure, aws
```