DATABASE_MAX_OVERFLOW=40

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_CLUSTER_ENABLED=true
REDIS_SENTINEL_ENABLED=true

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
