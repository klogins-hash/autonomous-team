# Security
JWT_SECRET=your-secret-key-here
JWT_ALGORITHM=HS256
JWT_EXPIRATION=86400
CORS_ORIGINS=http://localhost:3000,https://creativehub.com
API_RATE_LIMIT=1000

# Monitoring
SENTRY_DSN=
DATADOG_API_KEY=
